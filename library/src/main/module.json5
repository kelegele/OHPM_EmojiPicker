{
  "module": {
    "name": "library",
    "type": "har",
    "deviceTypes": [
      "default",
      "phone",
      "tablet",
      "2in1",
      "car"
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.VIBRATE",
        "reason": "$string:vibrate_permission_reason",
        "usedScene": {
          "abilities": [
            "EntryAbility"
          ],
          "when": "inuse"
        }
      }
    ]
  }
}